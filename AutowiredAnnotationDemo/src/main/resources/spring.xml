<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<context:annotation-config />

	<bean id="customerBean" class="com.bridgeLabz.AutowiredAnnotationDemo.Customer">
		<constructor-arg type="java.lang.String" value="kanjoose"></constructor-arg>
		<constructor-arg type="int" value="100"></constructor-arg>
	</bean>

	<bean id="personBean" class="com.bridgeLabz.AutowiredAnnotationDemo.Person">
		<constructor-arg type="java.lang.String" value="sejal"></constructor-arg>
		<constructor-arg type="int" value="1"></constructor-arg>

	</bean>
</beans>